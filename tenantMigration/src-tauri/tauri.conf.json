{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tenantmigration",
  "version": "0.1.0",
  "identifier": "com.tenantmigration.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:68993",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "tenantmigration",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/rOneLogo.png","icons/rOneLogo.ico"]
  }
}
